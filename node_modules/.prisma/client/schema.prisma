datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

generator client {
  provider = "prisma-client-js"
}

model FeedbackModel {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  status    String   @default("Создан")
  createdAt DateTime @default(now())
}

model Token {
  id         Int      @id @default(autoincrement())
  token      String
  expiration DateTime
}

model application {
  id        Int      @id @default(autoincrement())
  type      Int
  name      String?
  email     String?
  phone     String?
  from      String?
  to        String?
  desc      String?
  status    String   @default("Создан")
  createdAt DateTime @default(now())
}

model admin {
  id       Int    @id @default(autoincrement())
  login    String @unique
  password String
}
